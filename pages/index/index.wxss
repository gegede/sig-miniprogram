/**index.wxss**/
page {
  background-color: #fff;
}
.container .userinfo {
  padding-top: 30rpx;
  margin-bottom: 30rpx;
  line-height: 84rpx;
}
.container .userinfo image {
  vertical-align: middle;
  height: 84rpx;
  width: 84rpx;
  border-radius: 100%;
  margin-right: 30rpx;
}
.container .userinfo text {
  vertical-align: middle;
  font-size: 36rpx;
  line-height: 50rpx;
  font-weight: 400;
  color: #333;
}
.container swiper {
  height: 378rpx;
  box-shadow: 0px 5px 15px 0px rgba(225, 230, 238, 1);
}
.container swiper .slide-image {
  height: 378rpx;
  width: 690rpx;
}
.container .super-role {
  margin: 60rpx 0;
}
