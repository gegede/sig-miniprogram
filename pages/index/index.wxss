  margin-bottom: 30rpx;
  font-size: 36rpx;
  font-weight: 400;
  color: #333;
  line-height: 50rpx;
}
.container .list {
  color: #6d7278;
  font-size: 28rpx;
  font-weight: 400;
}
.container .list .item {
  padding: 30rpx;
  border-radius: 8px;
}
.container .list .item .name {
  font-size: 34rpx;
  color: #333;
  font-weight: 400;
  line-height: 48rpx;
  margin-top: 0;
}
.container .list .item view {
  margin-top: 20rpx;
  line-height: 40rpx;
}
.container .list .item view image {
  vertical-align: middle;
  width: 28rpx;
  height: 28rpx;
  margin-right: 22rpx;
}
.container .list .item view text {
  vertical-align: middle;
}
.container .list .item .desc {
  white-space: nowrap; 
  overflow: hidden;
  text-overflow:ellipsis;
}
.container .list .slot {
  display: flex;
  justify-content: flex-end;
  height: 100%;
  width: 135px;
}
.container .list .slot .get-addr {
  margin-left: 10px;
  flex-shrink: 1;
  width: 57.5px;
  padding: 0 10px;
  font-size: 17px;
  color: #fff;
  line-height: 24px;
  text-align: center;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #002fa7;
  border-radius: 8rpx;
  box-shadow: 0px 5px 20px 0px rgba(0, 47, 167, 0.28);
}
.container .list .slot .del-meeting {
  margin-left: 10px;
