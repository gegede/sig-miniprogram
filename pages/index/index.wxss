  color: #333;
  font-size: 36rpx;
  font-weight: 400;
  border-radius: 8rpx;
  box-shadow: 0px 5px 15px 0px rgba(225, 230, 238, 1);
  margin-bottom: 30rpx;
}
.container .super-role .row1 image {
  display: inline-block;
  width: 46rpx;
  height: 44rpx;
