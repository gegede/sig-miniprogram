<!--pages/sig/del-member.wxml-->
<view class="container">
  <van-checkbox-group value="{{ result }}" bind:change="onChange">
    <block wx:for="{{list}}" wx:key="index">
      <view class="del-item">
          <van-checkbox icon-size="46rpx" checked-color="#002fa7" name="{{item.name}}">
          <view class="inner">
            <image /><text>{{item.name}}</text>
          </view>
          </van-checkbox>
      </view>
    </block>
  </van-checkbox-group>
  <button class="del-btn">删除</button>
</view>

