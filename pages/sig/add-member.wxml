<!--pages/sig/add-member.wxml-->
<view class="container">
    <view class="input-wraper">
      <input placeholder="搜索成员" class="keyword"/>
      <image src="./../../static/common/search-icon.svg" class="search-icon"></image>
  </view>
  <view class="fill">{{list.length?'':'没有找到 '+keyword+'相关成员'}}</view>
  <van-checkbox-group value="{{ result }}" bind:change="onChange">
    <block wx:for="{{list}}" wx:key="index">
      <view class="add-item">
          <van-checkbox icon-size="46rpx" checked-color="#002fa7" name="{{item.name}}">
          <view class="inner">
            <image /><text>{{item.name}}</text>
          </view>
          </van-checkbox>
      </view>
    </block>
  </van-checkbox-group>
  <button class="add-btn">确定</button>
</view>
